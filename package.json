{
  "name": "nodewebkit_starterkit",
  "version": "0.0.0",
  "description": "NodeWebKit Starterkit",
  "main": "app/pages/Main.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Satit Rianpit <rianpit@gmail.com>",
  "license": "MIT",
  "locale": "th_TH",
  "single-instance": true,
  "window": {
    "show": true,
    "title": "My Application",
    "icon": "app/img/icon.png",
    "toolbar": true,
    "frame": true,
    "width": 1050,
    "height": 640,
    "position": "center",
    "min_width": 1050,
    "min_height": 640,
    "fullscreen": false
  },
  "webkit": {
    "plugin": true,
    "java": false,
    "page-cache": true
  },
  "chromium-args": "--lang=th_TH --child-clean-exit --disable-accelerated-video --disabled-application-cache --disabled-backing-store-limit",
  "devDependencies": {
    "gulp": "^3.8.10",
    "gulp-jade": "^0.10.0",
    "gulp-jshint": "^1.9.0",
    "gulp-watch": "^3.0.0",
    "gulp-less": "^3.0.1"
  },
  "dependencies": {
    "MD5": "^1.2.1",
    "jsonfile": "^2.0.0",
    "knex": "^0.7.3",
    "lodash": "^2.4.1",
    "moment": "^2.9.0",
    "mysql": "^2.5.4"
  }
}
